                         "// player sideChat format [""GLX_System.FSM > Random > %1 > %2"", _group, _random];" \n
                         "" \n
                         "if (GLX_Debug select 1) then" \n
                         "{" \n
                         "	_marker = [_group, ""colorWhite""] call (GLX_Marker_F select 0);" \n
